# Server configuration
PORT=8080

# OpenTelemetry and Prometheus configuration
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_SERVICE_NAME=fizzbuzz-server
OTEL_RESOURCE_ATTRIBUTES=service.version=1.0.0,deployment.environment=development

# Prometheus configuration
PROMETHEUS_ENABLED=true
PROMETHEUS_ENDPOINT=/metrics
PROMETHEUS_PUSH_GATEWAY=http://localhost:9091
PROMETHEUS_PUSH_INTERVAL=10s

# Add other environment variables as needed